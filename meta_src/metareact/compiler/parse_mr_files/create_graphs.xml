<?xml version="1.0"?>
<root>
  <info>
    <name> create_graphs</name>
    <description> It creates an object representation of graph present in the mr file. </description>
  </info>
  <inputs>
    <input generated="true" name="mr_file_paths"/>
    <input generated="true" name="srcodes"/>
    <input name="srcodes" origin="find_end_points" generated="true"/>
    <input name="srcodes" origin="find_functions" generated="true"/>
    <input name="srcodes" origin="path_traversal" generated="true"/>
    <input name="mr_file_paths" origin="path_traversal" generated="true"/>
  </inputs>
  <outputs>
    <output generated="true" name="graphs"/>
  </outputs>
  <graph generated="true">
    <node fn_name="find_functions">
      <output name="function_names">
        <end_point fn_name="find_end_points"/>
      </output>
      <input name="srcodes"/>
    </node>
    <node fn_name="find_end_points">
      <output name="end_points">
        <end_point fn_name="path_traversal"/>
      </output>
      <input name="srcodes"/>
      <input name="function_names"/>
    </node>
    <node fn_name="path_traversal">
      <input name="srcodes"/>
      <input name="mr_file_paths"/>
      <input name="end_points"/>
    </node>
  </graph>
</root>
