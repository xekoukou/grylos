<?xml version="1.0"?>
<root>
    <info>
        <name> flatten_graph </name>
        <description> It creates a flattened graph. </description>
    </info>
    <inputs>
        <input name="source_path">
            <origin origin_name="source_path" origin_location="find_starting_points"/>
            <origin origin_name="source_path" origin_location="create_flattened_graph"/>
        </input>
        <input name="fs">
            <origin origin_name="fs" origin_location="find_starting_points"/>
            <origin origin_name="fs" origin_location="create_flattened_graph"/>
        </input>
        <input name="path">
            <origin origin_name="path" origin_location="find_starting_points"/>
        </input>
        <input name="cheerio">
            <origin origin_name="cheerio" origin_location="find_starting_points"/>
            <origin origin_name="cheerio" origin_location="create_flattened_graph"/>
        </input>
        <input name="xml_files">
            <origin origin_name="xml_files" origin_location="find_starting_points"/>
            <origin origin_name="xml_files" origin_location="create_flattened_graph"/>
        </input>
    </inputs>
    <graph generated="true">
        <node fn_name="find_starting_points">
            <output name="starting_points">
                <end_point fn_name="create_flattened_graph"/>
            </output>
        </node>
    </graph>
    <outputs>
        <output name="flattened_graph" generated="true">
            <origin origin_name="flattened_graph" origin_location="create_flattened_graph" generated="true"/>
        </output>
    </outputs>
</root>
