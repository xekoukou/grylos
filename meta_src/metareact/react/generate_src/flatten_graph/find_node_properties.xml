<?xml version="1.0"?>
<root>
    <info>
        <name> find_node_properties </name>
        <description> It creates a list with all the node properties of all the nodes. </description>
    </info>
    <inputs>
        <input name="source_path"> </input>
        <input name="fs"/>
        <input name="path"/>
        <input name="cheerio"/>
        <input name="single_threaded"/>
        <input name="check_only_side_effects_exist_dep" dependency="true"/>
        <input name="xml_files" side-effect="true"/>
    </inputs>
    <outputs>
        <output name="error" side-effect="true">
            <description> It returns an error when we have set the asynchronous property on a &quot;subgraph&quot; node.</description>
        </output>
        
    <output generated="true" name="node_properties"/></outputs>
</root>
